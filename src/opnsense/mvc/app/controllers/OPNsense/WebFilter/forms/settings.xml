        <form>
    <tab id="webfilter-general" description="Settings">
        <subtab id="webfilter-general-settings" description="General">
            <field>
                <id>webfilter.general.enabled</id>
                <label>Enable Web Filter</label>
                <type>checkbox</type>
                <help>Enable WebFilter.</help>
            </field>
            <field>
                <id>webfilter.general.ldapbinddn</id>
                <label>LDAP Bind DN</label>
                <type>text</type>
                <help>user DistringuishedName to connect to ldap server.</help>
            </field>
            <field>
                <id>webfilter.general.ldapbindpass</id>
                <label>LDAP Bind Password</label>
                <type>password</type>
                <help>password for the ldap binding user above. not not use special characters like @.</help>
            </field>
            <field>
                <id>webfilter.general.ldapcachetime</id>
                <label>LDAP Result Cached time</label>
                <type>text</type>
                <help>How much time to cache ldap results</help>
            </field>
            <field>
                <id>webfilter.general.sguardprocs</id>
                <label>Number of processes</label>
                <type>text</type>
                <help>Max number of WebFilter processes to spawn (between 5-50)</help>
            </field>
            <field>
                <id>webfilter.general.categoriesurl</id>
                <label>Blacklist Categories URL</label>
                <type>text</type>
                <help>URL to fetch the blacklist categories</help>
            </field>
            <field>
                <id>webfilter.general.categories</id>
                <label>Enabled Categories</label>
                <type>text</type>
                <type>select_multiple</type>
                <style>tokenize</style>
                <nbDropdownElements>300</nbDropdownElements>
                <help><![CDATA[Select categories to enable in rules.]]></help>
            </field>
            <field>
                <id>webfilter.general.urlerror</id>
                <label>Default Redirect URL</label>
                <type>text</type>
                <help>example: 302:https://192.168.1.1/sgerror.php?url=403%20&a=%a&n=%n&i=%i&s=%s&t=%t&u=%u</help>
            </field>
        </subtab>
    </tab>
    <activetab>webfilter-general-settings</activetab>
</form>
